<div class="row">
  <div class="col-xs-4 col-xs-offset-4">
    <h1>Sent Messages</h1>
    <ul>
      <% @messages.each do |m| %>
        <li><%= link_to m.body, message_path(m) %></li>
      <%end%>
    </ul>
  </div>
</div>

<div class="row">
  <div class="col-xs-4 col-xs-offset-4">
    <%= link_to "New Message", new_message_path, class: "btn-lg btn-info"%>    
  </div>
</div>
